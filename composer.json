{
	"name": "codekandis/persistence",
	"description": "`codekandis/persistence` is a library providing database connector and repository interfaces and classes.",
	"keywords": [
		"php",
		"library",
		"persistence",
		"interfaces",
		"classes"
	],
	"minimum-stability": "stable",
	"prefer-stable": true,
	"license": "MIT",
	"authors": [
		{
			"name": "Christian Ramelow",
			"email": "info@codekandis.net",
			"homepage": "https://codekandis.net",
			"role": "Developer"
		}
	],
	"support": {
		"email": "info@codekandis.net",
		"issues": "https://github.com/codekandis/persistence/issues",
		"docs": "https://github.com/codekandis/persistence/blob/master/README.md",
		"source": "https://github.com/codekandis/persistence/tree/master"
	},
